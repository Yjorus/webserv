server {
	listen 2000;
	server_name	Wahoo.com;
	root	rootdir/;
	host	127.0.0.1;
	index	index.html;
	client_max_body_size 1000000000;
	#ooooooo
	listing on;
	error_pages
	404	/error/404.html
	400	folder/file2.html;
	location / {#WOW
		index index.html;
		methods	GET POST;
	}
	location /test/ {#WOW
		listing on;
		index test.html;
		methods	GET POST;
	}

	location /lmao/ {
		listing on;
		index lol.html;
		proxy /test/lol/;
		methods GET;
	}

